<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>

    <IsPackable>false</IsPackable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="NUnit" Version="3.12.0" />
    <PackageReference Include="NUnit3TestAdapter" Version="3.16.1" />
    <PackageReference Include="Microsoft.NET.Test.Sdk" Version="16.5.0" />
    <PackageReference Include="System.Collections" Version="4.3.0" />
    <PackageReference Include="System.Data.SqlClient" Version="4.8.2" />
    <PackageReference Include="System.Net.Security" Version="4.3.2" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Features\Create.feature.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Create.feature</DependentUpon>
    </Compile>
    <Compile Update="Features\Delete.feature.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Delete.feature</DependentUpon>
    </Compile>
    <Compile Update="Features\Update.feature.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Update.feature</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Update="Features\Create.feature">
      <Generator>SpecFlowSingleFileGenerator</Generator>
      <LastGenOutput>Create.feature.cs</LastGenOutput>
    </None>
    <None Update="Features\Delete.feature">
      <Generator>SpecFlowSingleFileGenerator</Generator>
      <LastGenOutput>Delete.feature.cs</LastGenOutput>
    </None>
    <None Update="Features\Update.feature">
      <Generator>SpecFlowSingleFileGenerator</Generator>
      <LastGenOutput>Update.feature.cs</LastGenOutput>
    </None>
  </ItemGroup>

</Project>
